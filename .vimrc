execute pathogen#infect()

syntax on
set number
set mouse=a
set cursorline
set backspace=2
set noerrorbells
set novisualbell
set visualbell t_vb=
set encoding=utf-8
filetype plugin indent on
set tabstop=4
set shiftwidth=4
let g:two_firewatch_italics=1
highlight OverLength ctermbg=red ctermfg=white guibg=#592929
